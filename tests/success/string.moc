void print(char* str) {
    asm("
        LOAD (1) -1[LB]
        print_loop:
        LOAD (1) 3[LB]
        LOADI (1)
        SUBR Cout
        LOAD (1) 3[LB]
        LOADL 1
        SUBR IAdd
        STORE (1) 3[LB]
        LOAD (1) 3[LB]
        LOADI (1)
        LOADL 0
        SUBR IEq
        JUMPIF (0) print_loop
    ");
}

void main() {
    char* message = "Hello world";
    *message = 'A';
    print(message);
}
